  <!-- use redirect tag for main navigation rules (corresponding to
  tab page elements) so that URLs for these actions will be known to
  the browser and will behave correctly when bookmarked -->

  <navigation-rule>
    <navigation-case>
      <from-outcome>go_login</from-outcome>
      <to-view-id>/facelets/authentication/login.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <navigation-case>
      <from-outcome>go_logout</from-outcome>
      <to-view-id>/facelets/authentication/logout.jsf</to-view-id>
    </navigation-case>
  </navigation-rule>

  <navigation-rule>
    <navigation-case>
      <from-outcome>go_user</from-outcome>
      <to-view-id>/admin/userInfo.jsf</to-view-id>
