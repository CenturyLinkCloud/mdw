<?xml version="1.0" encoding="UTF-8"?>
<faces-config xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd" version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance">
        
  <name>MDWHub</name>
    
  <application>
    <message-bundle>MdwHubUiMessages</message-bundle>
    <locale-config>
      <default-locale>en_US</default-locale>
      <supported-locale>en</supported-locale>
    </locale-config>
    <resource-handler>com.centurylink.mdw.hub.jsf.ResourceHandler</resource-handler>
    
    <!-- TODO refactor based on Manoj's work -->
    <el-resolver>com.centurylink.mdw.web.MDWELResolver</el-resolver> 
    <!-- TODO check if this is necessary -->
    <navigation-handler>com.centurylink.mdw.hub.jsf.NavigationHandler</navigation-handler>
  </application>

  <factory>
    <view-declaration-language-factory>com.centurylink.mdw.hub.jsf.ViewDeclarationLanguageFactory</view-declaration-language-factory>
    <facelet-cache-factory>com.centurylink.mdw.hub.jsf.FaceletCacheFactory</facelet-cache-factory>
    <external-context-factory>com.centurylink.mdw.web.jsf.view.MdwExternalContextFactory</external-context-factory>
    <exception-handler-factory>com.centurylink.mdw.hub.jsf.UIExceptionHandlerFactory</exception-handler-factory>
  </factory>
  
  <lifecycle>
    <phase-listener>com.centurylink.mdw.hub.jsf.UiPhaseListener</phase-listener>
  </lifecycle>  
  
  <component>
    <component-type>com.centurylink.mdw.hub.jsf.component.HtmlTag</component-type>
