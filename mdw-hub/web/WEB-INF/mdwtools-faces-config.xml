  <managed-bean>
    <managed-bean-name>fileView</managed-bean-name>
    <managed-bean-class>com.centurylink.mdw.web.filepanel.FileView</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
  </managed-bean>
  <!-- default managed property values can be overridden in config file -->
  <managed-bean>
    <managed-bean-name>filepanelDirView</managed-bean-name>
    <managed-bean-class>com.centurylink.mdw.web.filepanel.DirView</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>rootDirectories</property-name>
      <value>./logs,./data/log,/tmp/argos/log,~/.argos/log</value>
    </managed-property>
    <managed-property>
      <property-name>rootDirPropertyName</property-name>
      <value>mdw.filepanel.root.dirs</value>
    </managed-property>
    <managed-property>
      <property-name>binaryPatterns</property-name>
      <value>*.zip,*.gz,*.gif,*.jpg,*.png,*.jks</value>
    </managed-property>
  </managed-bean>
  <managed-bean>
    <managed-bean-name>propertyDirView</managed-bean-name>
    <managed-bean-class>com.centurylink.mdw.web.filepanel.DirView</managed-bean-class>
    <managed-bean-scope>session</managed-bean-scope>
    <managed-property>
      <property-name>rootDirectories</property-name>
      <value>./mdw/config,./CenturyLink/config,./Qwest/config,./conf,./etc,./cleartrust</value>
    </managed-property>
    <managed-property>
      <property-name>rootDirPropertyName</property-name>
      <value>mdw.filepanel.config.dirs</value>
    </managed-property>
    <managed-property>
      <property-name>binaryPatterns</property-name>
      <value>*.zip,*.gz,*.gif,*.jpg,*.png,*.jks</value>
    </managed-property>
    <managed-property>
      <property-name>editable</property-name>
      <value>true</value>
    </managed-property>
  </managed-bean>  
  <managed-bean>
    <managed-bean-name>fileGrep</managed-bean-name>
    <managed-bean-class>com.centurylink.mdw.web.filepanel.FileGrep</managed-bean-class>
