  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

  <!-- NOTE: modified for mdw-hub -->

<facelet-taglib>

  <namespace>http://www.centurylink.com/mdw/jsf</namespace>

  <!-- common java-implemented components -->
  <tag>
    <tag-name>html</tag-name>
    <component>
      <component-type>com.centurylink.mdw.hub.jsf.component.HtmlTag</component-type>
      <renderer-type>com.centurylink.mdw.hub.jsf.component.HtmlTagRenderer</renderer-type>
    </component>
  </tag>  
  <tag>
    <tag-name>ajaxCommandLink</tag-name>
    <component>
      <component-type>com.centurylink.mdw.hub.jsf.component.AjaxCommandLink</component-type>
      <renderer-type>com.centurylink.mdw.hub.jsf.component.AjaxCommandLinkRenderer</renderer-type>
    </component>
  </tag>  
  <tag>
    <tag-name>navMenu</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.NavigationMenu</component-type>
      <renderer-type>com.centurylink.mdw.hub.jsf.component.NavigationMenuRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>navLink</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.NavigationLink</component-type>
      <renderer-type>com.centurylink.mdw.hub.jsf.component.NavigationLinkRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>link</tag-name>
    <component>
