    </component>
  </tag>
  <tag>
    <tag-name>tab</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.Tab</component-type>
      <renderer-type>com.centurylink.mdw.hub.jsf.component.TabRenderer</renderer-type>
