  <!-- common facelets components -->
  <tag>
    <tag-name>blank</tag-name>
    <source>../components/blank.xhtml</source>
  </tag>
  <tag>
    <tag-name>expandable</tag-name>
    <source>../components/expandable.xhtml</source>
  </tag>
  <tag>
    <tag-name>pickList</tag-name>
    <source>../components/pickList.xhtml</source>
  </tag>
  <tag>
    <tag-name>sysInfo</tag-name>
    <source>../components/sysInfo.xhtml</source>
  </tag>
  <tag>
    <tag-name>label</tag-name>
    <source>../components/label.xhtml</source>
  </tag>
  <tag>
    <tag-name>sectionLabel</tag-name>
    <source>../components/sectionLabel.xhtml</source>
  </tag>
  <tag>
    <tag-name>clearTrustLoginForm</tag-name>
    <source>../components/clearTrustLoginForm.xhtml</source>
  </tag>
  <tag>
    <tag-name>listFilter</tag-name>
    <source>../components/listFilter.xhtml</source>
  </tag>
  <tag>
    <tag-name>dateRange</tag-name>
    <source>../components/dateRange.xhtml</source>
  </tag>
  <tag>
