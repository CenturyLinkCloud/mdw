  </tag>
  <tag>
    <tag-name>timeInterval</tag-name>
    <source>../components/timeInterval.xhtml</source>
  </tag>
  
  <!-- converters -->
  <tag>
    <tag-name>convertDateTime</tag-name>
    <converter>
      <converter-id>dateTimeConverter</converter-id>
    </converter>
  </tag>
  
  <!-- validators -->
  <tag>
    <tag-name>validateDateRange</tag-name>
    <validator>
      <validator-id>dateRangeValidator</validator-id>
    </validator>
  </tag>
    
</facelet-taglib>