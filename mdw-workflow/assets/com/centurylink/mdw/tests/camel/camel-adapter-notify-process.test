// camel-adapter-notify-process 
//make sure camel-core bundle is active 
start process("CamelEventWait") {
	variables = [eventId: 2537]
}
sleep 45
start process("MDWTestCamelAdapter") {
	variables = [testRequest: asset("eventRequest.xml").text]
}
wait process("CamelEventWait") {
    timeout = 30
}
verify processes("MDWTestCamelAdapter", "CamelEventWait")