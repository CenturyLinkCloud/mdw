- id: A3
  name: Loop Back
  implementor: com.centurylink.mdw.workflow.activity.script.ScriptExecutorActivity
  attributes:
    RETURN_CODE: comp
    Rule: return "BACK"
    SCRIPT: Groovy
    WORK_DISPLAY_INFO: x=498,y=281,w=100,h=60
  transitions:
  - id: T7
    to: A4
