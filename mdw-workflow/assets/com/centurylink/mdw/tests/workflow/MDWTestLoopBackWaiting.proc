- id: A1
  name: Stop
  implementor: com.centurylink.mdw.workflow.activity.process.ProcessFinishActivity
  attributes:
    WORK_DISPLAY_INFO: x=146,y=431,w=60,h=40
- id: A4
  name: Loop Control
  implementor: com.centurylink.mdw.workflow.activity.script.ScriptExecutorActivity
  attributes:
    Expression: |-
      N = N + 1;
      if (N==3) {
          comp = "TEMP";
      } else if (N>5) {
          comp = "DONE";
      } else {
          comp = "RETRY";
      }
    RETURN_CODE: comp
    Rule: |-
      n++
      if (n == 3)
          return "TEMP"
      else if (n > 5)
          return "DONE"
      else
          return "BACK"
    SCRIPT: Groovy
    WORK_DISPLAY_INFO: x=176,y=259,w=100,h=60
  transitions:
  - id: T4
    to: A1
    event: FINISH
    resultCode: DONE
    attributes:
      TRANSITION_DISPLAY_INFO: lx=201,ly=375,type=Straight,xs=226&176,ys=319&431
  - id: T5
    to: A4
    event: FINISH
    resultCode: BACK
    attributes:
      TRANSITION_DELAY: '1'
      TRANSITION_DISPLAY_INFO: lx=242,ly=149,type=Curve,xs=226&247,ys=289&176
      TRANSITION_RETRY_COUNT: '3'
  - id: T6
    to: A3
    event: FINISH
    resultCode: TEMP
    attributes:
      TRANSITION_DISPLAY_INFO: lx=374,ly=205,type=Curve,xs=280&365&494,ys=255&230&277
variables:
  n:
