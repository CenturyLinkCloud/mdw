    DoNotNotifyCaller: 'false'
    WORK_DISPLAY_INFO: x=424,y=93,w=60,h=40
- id: A3
  name: Set Info
  implementor: com.centurylink.mdw.workflow.activity.script.ScriptExecutorActivity
  attributes:
    Rule: "procName = runtimeContext.getProcess().getName();\r\nprocVer = runtimeContext.getProcess().getVersionString(); "
    SCRIPT: Groovy
    WORK_DISPLAY_INFO: x=231,y=85,w=100,h=60
  transitions:
  - id: T2
    to: A2
    event: FINISH
    attributes:
      TRANSITION_DISPLAY_INFO: lx=378,ly=113,type=Elbow,xs=335&420,ys=113&113
variables:
  procName:
    type: java.lang.String
    category: OUTPUT
  procVer:
    type: java.lang.String
    category: OUTPUT
attributes:
  ArrowStyle: ArrowEnd
  NodeStyle: BoxIcon
  PROCESS_VISIBILITY: PUBLIC
  WORK_DISPLAY_INFO: x=27,y=24,w=172,h=27
