package com.centurylink.mdw.common.service;

import java.util.List;

import org.json.JSONException;
import org.json.JSONObject;

public class JsonList implements Jsonable {

    private List<? extends Jsonable> jsonables;
    public List<? extends Jsonable> getJsonables() { return jsonables; }

    public JsonList(List<? extends Jsonable> jsonables) {
        this.jsonables = jsonables;
    }

    @Override
    public JSONObject getJson() throws JSONException {
        return null;
    }

    @Override
    public String getJsonName() {
        if (jsonables != null && jsonables.size() > 0)
            return jsonables.get(0).getJsonName();
        return null;
    }

}
