<?xml version="1.0"?>
<!DOCTYPE facelet-taglib PUBLIC
  "-//Sun Microsystems, Inc.//DTD Facelet Taglib 1.0//EN"
  "http://java.sun.com/dtd/facelet-taglib_1_0.dtd">

<facelet-taglib>

  <namespace>http://www.centurylink.com/mdw/jsf</namespace>

  <!-- common java-implemented components -->
  <tag>
    <tag-name>navMenu</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.NavigationMenu</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.NavigationMenuRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>navLink</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.NavigationLink</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.NavigationLinkRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>tabPanel</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.TabPanel</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.TabPanelRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>tab</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.Tab</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.TabRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>dataScroller</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.DataScroller</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.DataScrollerRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>dataTable</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.DataTable</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.DataTableRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>javascriptInclude</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.Javascript</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.JavascriptRenderer</renderer-type>
    </component>
  </tag>  
  <tag>
    <tag-name>validator</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.Validator</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.ValidatorRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>filter</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.Filter</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.FilterRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>criteria</tag-name>
    <component>
      <component-type>com.centurylink.mdw.web.jsf.components.Criteria</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.CriteriaRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>panelContainer</tag-name>
    <component>
      <component-type>org.apache.myfaces.HtmlPanelGroup</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.PanelContainerRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>togglePanel</tag-name>
    <component>
      <component-type>org.richfaces.SimpleTogglePanel</component-type>
      <renderer-type>com.centurylink.mdw.web.jsf.renderers.TogglePanelRenderer</renderer-type>
    </component>
  </tag>
  <tag>
    <tag-name>subform</tag-name>
    <component>
      <component-type>org.apache.myfaces.SubForm</component-type>
      <renderer-type>org.apache.myfaces.SubForm</renderer-type>
    </component>
  </tag>  
  
  <!-- common facelets components -->
  <tag>
    <tag-name>blank</tag-name>
    <source>../../components/blank.xhtml</source>
  </tag>
  <tag>
    <tag-name>expandable</tag-name>
    <source>../../components/expandable.xhtml</source>
  </tag>
  <tag>
    <tag-name>pickList</tag-name>
    <source>../../components/pickList.xhtml</source>
  </tag>
  <tag>
    <tag-name>sysInfo</tag-name>
    <source>../../components/sysInfo.xhtml</source>
  </tag>
  <tag>
