    <source>../../components/dateRange.xhtml</source>
  </tag>
  <tag>
    <tag-name>date</tag-name>
    <source>../../components/date.xhtml</source>
  </tag>
  <tag>
    <tag-name>list</tag-name>
    <source>../../components/list.xhtml</source>
  </tag>
  <tag>
    <tag-name>filterPrefs</tag-name>
    <source>../../components/filterPrefs.xhtml</source>
  </tag>
  <tag>
