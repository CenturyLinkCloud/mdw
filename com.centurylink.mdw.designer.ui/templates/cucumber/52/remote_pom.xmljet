<%@ jet package="translated" imports="java.util.* com.centurylink.mdw.plugin.project.model.* com.centurylink.mdw.plugin.preferences.model.*"
        class="OsgiPomXmlEmitter52" %>
<% Map map = (Map) argument;
   WorkflowProject workflowProject = (WorkflowProject) map.get("model");
   MdwSettings mdwSettings = (MdwSettings) map.get("settings");%>
<?xml version="1.0" encoding="UTF-8"?>
<project xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://maven.apache.org/POM/4.0.0" xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 http://maven.apache.org/maven-v4_0_0.xsd">

  <organization>
    <name>CenturyLink</name>
    <url>https://github.com/CenturyLinkCloud/mdw</url>
  </organization>

  <modelVersion>4.0.0</modelVersion>

  <properties>
    <bundle.symbolicName><%=workflowProject.getDefaultSourceCodePackage()%></bundle.symbolicName>
    <bundle.namespace><%=workflowProject.getDefaultSourceCodePackage()%></bundle.namespace>
    <res.dir><%=workflowProject.getOsgiSettings().getResourceDir()%></res.dir>
    <out.dir><%=workflowProject.getOsgiSettings().getOutputDir()%></out.dir>
    <mdw.version><%=workflowProject.getMdwVersion()%></mdw.version>
    <USER_NAME>${user.name}</USER_NAME>
  </properties>

  <groupId><%=workflowProject.getOsgiSettings().getGroupId()%></groupId>
  <artifactId><%=workflowProject.getOsgiSettings().getArtifactId()%></artifactId>
  <version>1.0.01</version>
  
  <name>${bundle.symbolicName}</name>
  <description><%=workflowProject.getSourceProjectName()%></description>

  <packaging>bundle</packaging>

  <repositories>
    <repository>
      <id>mdw.dev.repo</id>
      <url><%=mdwSettings.getMdwReleasesUrl()%></url>
    </repository>
  </repositories>

  <build>
    <pluginManagement>
      <plugins>
        <plugin>
          <groupId>org.apache.felix</groupId>
          <artifactId>maven-bundle-plugin</artifactId>
          <version>2.3.5</version>
        </plugin>
        <plugin>
          <groupId>org.ops4j</groupId>
          <artifactId>maven-pax-plugin</artifactId>
          <version>1.5</version>
          <extensions>true</extensions>
        </plugin>
        
        <!-- the following plugin stores Eclipse m2e settings only (it has no influence on the build) -->
        <plugin>
          <groupId>org.eclipse.m2e</groupId>
          <artifactId>lifecycle-mapping</artifactId>
          <version>1.0.0</version>
          <configuration>
            <lifecycleMappingMetadata>
              <pluginExecutions>
                <pluginExecution>
                  <pluginExecutionFilter>
                    <groupId>org.apache.cxf</groupId>
                    <artifactId>cxf-codegen-plugin</artifactId>
                    <versionRange>[2.3.2,)</versionRange>
                    <goals>
                      <goal>wsdl2java</goal>
                    </goals>
                  </pluginExecutionFilter>
                  <action>
                    <ignore></ignore>
                  </action>
                </pluginExecution>
              </pluginExecutions>
            </lifecycleMappingMetadata>
          </configuration>
        </plugin>
      </plugins>
    </pluginManagement>
  
    <resources>
      <resource>
        <directory>${res.dir}</directory>
        <excludes>
          <exclude>deploy/**/*</exclude>
        </excludes>
      </resource>
      <resource>
        <directory>${res.dir}/deploy/config</directory>
        <filtering>true</filtering>
        <targetPath>${server.dir}/etc</targetPath>
        <includes>
          <include>com.centurylink.mdw.cfg</include>
        </includes>
      </resource>      
    </resources>
    
    <sourceDirectory>src/main/java</sourceDirectory>
    <scriptSourceDirectory>scripts</scriptSourceDirectory>
    <testSourceDirectory>src/test/java</testSourceDirectory>
    <directory>${out.dir}</directory>
    <outputDirectory>build/classes</outputDirectory>
    <finalName>${project.artifactId}-${project.version}</finalName>
  
    <plugins>
      <plugin>
        <groupId>org.apache.maven.plugins</groupId>
        <artifactId>maven-compiler-plugin</artifactId>
        <version>2.3.2</version>
        <configuration>
          <source>1.6</source>
          <target>1.6</target>
          <outputDirectory>build/classes</outputDirectory>
        </configuration>
      </plugin>
      <plugin>
        <groupId>org.apache.felix</groupId>
        <artifactId>maven-bundle-plugin</artifactId>
        <extensions>true</extensions>
        <configuration>
          <instructions>
            <Export-Package>
              <%=workflowProject.getDefaultSourceCodePackage()%>.*
            </Export-Package>
            <Private-Package>
              <%=workflowProject.getDefaultSourceCodePackage()%>.bundle
            </Private-Package>
            <Import-Package>
              !com.centurylink.mdw.workflow.provider,
              !com.centurylink.mdw.listeners.provider,
              !com.centurylink.mdw.translator,
              com.centurylink.mdw.common.config.service,
              com.centurylink.mdw.common.provider,
              com.centurylink.mdw.jaxb,
              com.centurylink.mdw.services.bam,
              com.centurylink.mdw.monitor,
              com.qwest.mdw.java,
              com.qwest.mdw.xml,
              com.qwest.mdw.workflow.activity.types,
              com.qwest.mdw.workflow.event.types,
              com.qwest.mdw.workflow.activity.impl,
              com.qwest.mdw.common.translator,
              com.qwest.mdw.common.translator.base,
              com.qwest.mdw.common.exception,
              com.qwest.mdw.model.value.activity,
              com.qwest.mdw.xmlSchema,
              com.qwest.mdw.xmlSchema.impl,
              org.apache.xmlbeans.impl.schema,
              org.apache.commons.codec.binary,
              noNamespace.impl,
              javax.tools,
              javax.xml.bind,
              javax.xml.bind.annotation.adapters,
              javax.xml.bind.attachment,
              javax.xml.validation,
              javax.xml.stream,
              javax.xml.transform,
              javax.xml.namespace,
              javax.xml.xpath,
              org.w3c.dom,
              org.xml.sax,
              META-INF.cxf,
              *
            </Import-Package>
            <DynamicImport-Package>
              schemaorg_apache_xmlbeans.*,
              org.springframework.osgi.*,
              org.springframework.beans,
              org.springframework.beans.factory,
              org.springframework.beans.*,
              org.springframework.aop,
              org.springframework.aop.*,
              org.aopalliance.aop,              
              org.apache.cxf.*
            </DynamicImport-Package>
            <Bundle-Activator><%=workflowProject.getDefaultSourceCodePackage()%>.bundle.WorkflowBundleActivator</Bundle-Activator>
            <Bundle-ClassPath>., lib/mdw-base-${mdw.version}.jar</Bundle-ClassPath>
          </instructions>
        </configuration>
      </plugin>
    </plugins>
  </build>
  
