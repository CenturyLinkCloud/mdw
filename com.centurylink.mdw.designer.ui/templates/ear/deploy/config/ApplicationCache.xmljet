  <Property Name="RefreshMe">true</Property>
</Cache>
<Cache Name="DroolsKnowledgeBaseCache" Type="ReadOnly">
  <Property Name="ClassName">com.qwest.mdw.common.cache.impl.DroolsKnowledgeBaseCache</Property>
