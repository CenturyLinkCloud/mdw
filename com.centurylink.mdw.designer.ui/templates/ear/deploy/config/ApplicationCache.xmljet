if (workflowProject.checkRequiredVersion(5, 2))
{
%>
<Cache Name="VariableTypeCache" Type="ReadOnly">
  <Property Name="ClassName">com.qwest.mdw.common.cache.impl.VariableTypeCache</Property>
</Cache>
