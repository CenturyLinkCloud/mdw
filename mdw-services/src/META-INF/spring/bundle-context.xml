<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xmlns:osgi="http://www.springframework.org/schema/osgi"
  xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans-2.5.xsd
                      http://www.springframework.org/schema/osgi http://www.springframework.org/schema/osgi/spring-osgi-1.0.xsd">

  <bean id="processManager"
    class="com.centurylink.mdw.services.process.ProcessManagerBean" />
  <osgi:service ref="processManager">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.ProcessManager</value>
    </osgi:interfaces>
  </osgi:service>

  <bean id="eventManager"
    class="com.centurylink.mdw.services.event.EventManagerBean" /> 
  <osgi:service ref="eventManager">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.EventManager</value>
    </osgi:interfaces>
  </osgi:service>

  <bean id="userManager"
    class="com.centurylink.mdw.services.user.UserManagerBean" /> 
  <osgi:service ref="userManager">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.UserManager</value>
    </osgi:interfaces>
  </osgi:service>

  <bean id="taskManager"
    class="com.centurylink.mdw.services.task.TaskManagerBean" />
  <osgi:service ref="taskManager">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.TaskManager</value>
    </osgi:interfaces>
  </osgi:service>
  
  <bean id="orderServices" class="com.centurylink.mdw.services.order.OrderServicesImpl" />
  <osgi:service ref="orderServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.OrderServices</value>
    </osgi:interfaces>
  </osgi:service>
  
  <bean id="requestServices" class="com.centurylink.mdw.services.request.RequestServicesImpl" />
  <osgi:service ref="requestServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.RequestServices</value>
    </osgi:interfaces>
  </osgi:service>
    
  <bean id="processServices" class="com.centurylink.mdw.services.workflow.ProcessServicesImpl" />
  <osgi:service ref="processServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.ProcessServices</value>
    </osgi:interfaces>
  </osgi:service>

  <bean id="workflowServices" class="com.centurylink.mdw.services.workflow.WorkflowServicesImpl" />
  <osgi:service ref="workflowServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.WorkflowServices</value>
    </osgi:interfaces>
  </osgi:service>
  
  <bean id="assetServices" class="com.centurylink.mdw.services.asset.AssetServicesImpl" />
  <osgi:service ref="assetServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.AssetServices</value>
    </osgi:interfaces>
  </osgi:service>

  <bean id="solutionServices" class="com.centurylink.mdw.services.project.SolutionServicesImpl" />
  <osgi:service ref="solutionServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.SolutionServices</value>
    </osgi:interfaces>
  </osgi:service>

  <bean id="testingServices" class="com.centurylink.mdw.services.asset.TestingServicesImpl" />
  <osgi:service ref="testingServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.TestingServices</value>
    </osgi:interfaces>
  </osgi:service>
  
  <bean id="historyServices" class="com.centurylink.mdw.services.history.HistoryServicesImpl" />
  <osgi:service ref="historyServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.HistoryServices</value>
    </osgi:interfaces>
  </osgi:service>
   
  <bean id="externalMessageServices" class="com.centurylink.mdw.services.workflow.ExternalMessageServicesImpl" />
  <osgi:service ref="externalMessageServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.ExternalMessageServices</value>
    </osgi:interfaces>
  </osgi:service>

  <bean id="userServices" class="com.centurylink.mdw.services.user.UserServicesImpl" />
  <osgi:service ref="userServices">
    <osgi:interfaces>
      <value>com.centurylink.mdw.services.UserServices</value>
    </osgi:interfaces>
  </osgi:service>

  <!-- cache registration -->
  <bean id="TaskCache" class="com.centurylink.mdw.services.dao.task.cache.TaskTemplateCache" />
  <osgi:service ref="TaskCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="TaskCache"/>
    </osgi:service-properties>    
  </osgi:service>
  <bean id="VariableTypeCache" class="com.centurylink.mdw.common.cache.impl.VariableTypeCache" />
  <osgi:service ref="VariableTypeCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="VariableTypeCache"/>
    </osgi:service-properties>    
  </osgi:service>
  <bean id="ExternalEventCache" class="com.centurylink.mdw.services.event.ExternalEventCache" />
  <osgi:service ref="ExternalEventCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="ExternalEventCache"/>
    </osgi:service-properties>    
  </osgi:service>
  <bean id="TaskCategoryCache" class="com.centurylink.mdw.services.dao.task.cache.TaskCategoryCache" />
  <osgi:service ref="TaskCategoryCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="TaskCategoryCache"/>
    </osgi:service-properties>    
  </osgi:service>
  <bean id="RuleCache" class="com.centurylink.mdw.services.mbeng.RuleCache" />
  <osgi:service ref="RuleCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="RuleCache"/>
      <entry key="capacity" value="64"/>
    </osgi:service-properties>    
  </osgi:service>
  <bean id="TemplateCache" class="com.centurylink.mdw.common.cache.impl.TemplateCache" />
  <osgi:service ref="TemplateCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="TemplateCache"/>
    </osgi:service-properties>    
  </osgi:service>
  <bean id="RuleSetCache" class="com.centurylink.mdw.common.cache.impl.RuleSetCache" />
  <osgi:service ref="RuleSetCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="RuleSetCache"/>
      <entry key="PreInitialized" value="com.centurylink.mdw.script.GroovyExecutor" />
    </osgi:service-properties>    
  </osgi:service>
  <bean id="WebPageCache" class="com.centurylink.mdw.common.cache.impl.WebPageCache" />
  <osgi:service ref="WebPageCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="WebPageCache"/>
    </osgi:service-properties>    
  </osgi:service>
  <bean id="PackageVOCache" class="com.centurylink.mdw.common.cache.impl.PackageVOCache" />
  <osgi:service ref="PackageVOCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="PackageVOCache"/>
    </osgi:service-properties>    
  </osgi:service>
  <bean id="CompiledJavaCache" class="com.centurylink.mdw.java.CompiledJavaCache" />
  <osgi:service ref="CompiledJavaCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="CompiledJavaCache"/>
      <entry key="dependsOn" value="RuleSetCache,PackageVOCache"/>
      <!--
      <entry key="PreCompiled" value="MDW Testing.Acknowledger&#10;DonsTests.Employee" />
      -->
    </osgi:service-properties>    
  </osgi:service>
  <bean id="UserGroupCache" class="com.centurylink.mdw.services.dao.user.cache.UserGroupCache" />
  <osgi:service ref="UserGroupCache">
    <osgi:interfaces>
      <value>com.centurylink.mdw.common.provider.CacheService</value>
    </osgi:interfaces>
    <osgi:service-properties>
      <entry key="alias" value="UserGroupCache"/>
    </osgi:service-properties>    
  </osgi:service>
  


</beans>
