# Note, this is used by BLV to call web services from client javascript.
# For non SSL it should be something like (with fully qualified domain name)
# mdw.bam.services.url=http://lxdenvmtc099.dev.qintra.com:20001/mdw
# For SSL it should be something like  (with fully qualified domain name)
# Used by BAM to log BAM messages console
mdw.bam.url=log
#Use this URL to send BAM messages to external system
#mdw.bam.url=http://localhost:8080/mdw/Services/REST
# Name of realm to filter out BAM messages for BLV 
mdw.bam.realm=NETWORK

# Properties for BamToWebsocket.xml
# keystore file
mdw.bam.keyStore.file=etc/mdw.jks
# keystore file password
mdw.bam.keyStore.keyPassword=guest1
# broker url
mdw.bam.brokerURL=tcp://localhost:61616
# fromuri for the BAM messages (should be the activemq topic)
mdw.bam.route.fromuri=activemq:topic:blvTopic
# touri for the BAM messages (should be the websocket)
# for SSL websockets it should be similar to the below 
# mdw.bam.route.touri=websocket://0.0.0.0:9999/blvTopic?sendToAll=true&sslContextParametersRef=#sslContextParameters
# for non-SSL websockets, use the below
mdw.bam.route.touri=websocket://0.0.0.0:9090/blvTopic?sendToAll=true

