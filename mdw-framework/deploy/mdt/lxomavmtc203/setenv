WEB_EXT_PROP_DIR="/opt/apps/share/properties/mdwdev1.resources";
JAVA_OPTS="-server -Xdebug -Xrunjdwp:transport=dt_socket,server=y,suspend=n,address=8500 -Xms768m -Xmx1536m -XX:PermSize=64m -XX:MaxPermSize=256m -XX:+UseParallelGC -Dweb.external.propdir="$WEB_EXT_PROP_DIR" -Dcom.sun.management.jmxremote.port=8501 -Dcom.sun.management.jmxremote.ssl=false -Dcom.sun.management.jmxremote.authenticate=false";
PATH=$PATH:/opt/tib/MIDD50/tibrv/bin;
export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/opt/tib/MIDD50/tibrv/lib;
