PATH=/usr/sbin:$PATH
# The Following Variable is used to control the type of Apache binary used when starting your server.
#Prefork (/usr/sbin/httpd) is the default binary for Linux.  Worker (/usr/sbin/httpd.worker) is the Qwest Standard.
#HTTPD=httpd
HTTPD=httpd.worker
#Set Caching
#Cache will be cleared every scheduled bounce.
#Set CachePermanent=true only if Cache needs to be retained permanently
CachePermanent="false"
export PATH HTTPD CachePermanent

COMMAND="${HTTPD} -d /opt/apache/mdwadmin-50805/server -DSSL"
SleepTime="15"
